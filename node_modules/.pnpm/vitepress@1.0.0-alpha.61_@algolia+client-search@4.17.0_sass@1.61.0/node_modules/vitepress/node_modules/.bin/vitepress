#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tangxue/code/blog/node_modules/.pnpm/vitepress@1.0.0-alpha.61_@algolia+client-search@4.17.0_sass@1.61.0/node_modules/vitepress/bin/node_modules:/Users/tangxue/code/blog/node_modules/.pnpm/vitepress@1.0.0-alpha.61_@algolia+client-search@4.17.0_sass@1.61.0/node_modules/vitepress/node_modules:/Users/tangxue/code/blog/node_modules/.pnpm/vitepress@1.0.0-alpha.61_@algolia+client-search@4.17.0_sass@1.61.0/node_modules:/Users/tangxue/code/blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tangxue/code/blog/node_modules/.pnpm/vitepress@1.0.0-alpha.61_@algolia+client-search@4.17.0_sass@1.61.0/node_modules/vitepress/bin/node_modules:/Users/tangxue/code/blog/node_modules/.pnpm/vitepress@1.0.0-alpha.61_@algolia+client-search@4.17.0_sass@1.61.0/node_modules/vitepress/node_modules:/Users/tangxue/code/blog/node_modules/.pnpm/vitepress@1.0.0-alpha.61_@algolia+client-search@4.17.0_sass@1.61.0/node_modules:/Users/tangxue/code/blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
